{
  "name": "clsdemo",
  "version": "0.1.0",
  "description": "cls experiment",
  "main": "./dist/src/server.js",
  "private": true,
  "scripts": {
    "audit-checker": "check-audit",
    "audit-resolver": "resolve-audit",
    "build": "npm run build:clean && npm run build:dist",
    "build:clean": "rimraf dist",
    "build:dist": "ttsc",
    "lambda": "rimraf lambda && (cp -r dist lambda && cp package*.json ./lambda) && (cd ./lambda && npm ci --production)",
    "format": "npm run format:prettier",
    "format:prettier": "prettier --write '**/*.{ts,js,md,json}'",
    "test": "env TS_NODE_COMPILER=ttypescript mocha --config .mocharc-tsnode.yml",
    "test:cov": "env TS_NODE_COMPILER=ttypescript nyc npm run test",
    "dist:test": "mocha --config .mocharc-dist.yml",
    "dist:test:cov": "nyc npm run dist:test",
    "lint": "npm run lint:mdlint && npm run lint:eslint",
    "lint:mdlint": "[ -f .markdownlint.yaml ] && markdownlint .",
    "lint:eslint": "eslint --cache --ext .ts ./",
    "start:debug": "nodemon --inspect=5858 -r ./src/server.ts",
    "start:watch": "nodemon ./src/server.ts",
    "type-check": "ttsc --noEmit",
    "type-check:watch": "npm run type-check -- --watch"
  },
  "dependencies": {
    "@liammurray/orders-client-axios": "^1.0.4",
    "aws-sdk": "^2.663.0",
    "aws-xray-sdk": "^3.0.0",
    "aws-xray-sdk-core": "^3.0.0",
    "aws-xray-sdk-express": "^3.0.0",
    "axios": "^0.19.2",
    "body-parser": "^1.19.0",
    "cls-hooked": "^4.2.2",
    "dotenv": "^8.2.0",
    "env-var": "^6.1.1",
    "express": "^4.17.1",
    "got": "^11.0.2",
    "http-status-codes": "^1.4.0",
    "lodash": "^4.17.15",
    "mongodb": "^3.5.6",
    "mongoose": "^5.9.10",
    "pino": "^6.2.1",
    "uuid": "^7.0.3"
  },
  "devDependencies": {
    "@types/body-parser": "^1.19.0",
    "@types/chai": "^4.2.11",
    "@types/chai-as-promised": "^7.1.2",
    "@types/cls-hooked": "^4.3.0",
    "@types/eslint": "^6.8.0",
    "@types/eslint-plugin-prettier": "^3.1.0",
    "@types/express": "^4.17.6",
    "@types/lodash": "^4.14.150",
    "@types/markdownlint": "^0.13.0",
    "@types/mocha": "^7.0.2",
    "@types/mongodb": "^3.5.14",
    "@types/mongoose": "^5.7.14",
    "@types/node": "^13.13.4",
    "@types/nodemon": "^1.19.0",
    "@types/pino": "^6.0.0",
    "@types/prettier": "^2.0.0",
    "@types/sinon": "^9.0.0",
    "@types/sinon-chai": "^3.2.4",
    "@types/source-map-support": "^0.5.1",
    "@types/uuid": "^7.0.3",
    "@typescript-eslint/eslint-plugin": "^2.29.0",
    "@typescript-eslint/parser": "^2.29.0",
    "chai": "^4.2.0",
    "chai-as-promised": "^7.1.1",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-mocha": "^6.3.0",
    "eslint-plugin-prettier": "^3.1.3",
    "eslint-plugin-promise": "^4.2.1",
    "markdownlint": "^0.20.2",
    "markdownlint-cli": "^0.22.0",
    "mocha": "^7.1.2",
    "nodemon": "^2.0.3",
    "npm-audit-resolver": "^2.2.0",
    "nyc": "^15.0.1",
    "prettier": "^2.0.5",
    "sinon": "^9.0.2",
    "sinon-chai": "^3.5.0",
    "source-map-support": "^0.5.19",
    "ts-mocha": "^7.0.0",
    "ts-node": "^8.9.1",
    "ttypescript": "^1.5.10",
    "typescript": "^3.8.3",
    "typescript-eslint": "0.0.1-alpha.0",
    "typescript-transform-paths": "^1.1.14"
  },
  "repository": {
    "type": "git",
    "url": "na"
  },
  "author": "Joe Blow",
  "license": "",
  "readme": "./README.md"
}
